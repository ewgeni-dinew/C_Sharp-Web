@model string
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@{
    ViewData["Title"] = "Error";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Styles {
    <partial name="_SingleStylesPartial" />
}
<div class="container single_product_container">
    <h2 class="text-danger">
        Error
    </h2>
    <h4 class="text-danger">@Model</h4>
    <a class="btn btn-primary mr-3" onclick="javascript:goBack()" style="color:white">Go Back</a>
    <a class="btn btn-primary" asp-controller="Home" asp-action="Index">Home</a>
</div>

@section Scripts{
    <script type="text/javascript">
        function goBack() {
            window.history.back();
        }
    </script>
}
